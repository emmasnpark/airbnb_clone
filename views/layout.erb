<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Hello World</title>
  <link rel='stylesheet' href='/stylesheets/main.css'>
</head>
<body>
  <h1><%= session[:user_id] %></h1>
  <nav>
    <div class="logo">
      <a href="/"><img src="https://i.imgur.com/3ouKOac.jpg?1" width="50px", height="50px" alt=""></a>
    </div>
    <div class="nav_right">
      <a href="/users/become_a_host">Become a host</a>
      <% if !(session[:user_id]) %>
      <a href="/login/signup">Sign up</a>
      <a href="/login/signin">Sign in</a>
      <% end %>
    </div>
      <% if session[:user_id] %>
      <form class="" action="/session" method="post">
        <input type="hidden" name="_method" value="delete">
        <button>Log out</button>
      </form>
      <% end %>

  </nav>
  <%= yield %>

  <footer>
    <hr>
    <p>Copyright &copy; 2018.</p>
  </footer>

</body>
</html>
